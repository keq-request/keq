{
  "name": "{{ package.name }}",
  "description": "{{ package.name }}",
  "version": "{{ package.version}}",

  "exports": {
    "./request": {
      "types": "./dist/esm/request.d.ts",
      "import": "./dist/esm/request.js",
      "require": "./dist/umd/request.js"
    },
    "./schemas": {
      "types": "./dist/esm/schemas/index.d.ts",
      "import": "./dist/esm/schemas/index.js",
      "require": "./dist/umd/schemas/index.js"
    },
    "./operations": {
      "types": "./dist/esm/operations/index.d.ts",
      "import": "./dist/esm/operations/index.js",
      "require": "./dist/umd/operations/index.js"
    }
  },

  "keywords": ["opendoc", "request"],

  "scripts": {
    "build": "npm run clean && ./build.sh",
    "clean": "rm -rf ./dist/*",
    "prepublishOnly": "npm run build"
  },

  "devDependencies": {
    "@types/node": "^20.11.5",
    "keq": "^2.3.0",
    "typescript": "^5.3.3"
  },

  "dependencies": {
  },

  "peerDependencies": {
    "keq": "^2"
  }
}
